@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->

    <!-- Bootstrap -->
    <link href="~/Css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Css/font-awesome.min.css" rel="stylesheet" />
    <!-- Custom -->
    <link href="~/Css/customCss.css" rel="stylesheet" />
    <link href="~/Css/richTextEditor.css" rel="stylesheet" />
    <!-- nanoGallery plugin -->
    <link href="~/Css/nanogallery.min.css" rel="stylesheet" type="text/css">
    

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="~/media/Images/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <title>@CurrentPage.pageName | @CurrentPage.siteName</title>

</head>
<body>
    <header>
        <nav class="navbar navbar-fixed-top cbp-af-header navbar-inverse" role="navigation">
            <div class="container-fluid">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-left">
                        <li><a href="~/">Home</a></li>
                        <li class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown" data-target="#" href="~/cowboy-grill-saloon/">
                                Cowboy Grill & Saloon<span class="caret"></span></a>
                            <!-- dropdown -->
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu">
                                <li><a href="~/cowboy-grill-saloon/">Cowboy Grill & Saloon</a></li>
                                <li class="divider"></li>
                                <li><a href="~/menu-kort/">Menu Kort</a></li>
                                <li><a href="~/bestil-bord/">Bestil Bord</a></li>
                                <li><a href="~/information/">Information</a></li>
                                </ul>
                        <li class="dropdown">
                            <a href="~/cowboy-fest/" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="False">Cowboy Fest<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="~/cowboy-fest/">Cowboy Fest</a></li>
                                <li class="divider"></li>
                                <li><a href="~/the-renegades/">The Renegades 99,-</a></li>
                                <li><a href="~/the-buffalos/">The Buffalos 349,-</a></li>
                                <li><a href="~/the-cowboys/">The Cowboys 649,-</a></li>
                                <li><a href="~/the-marshalls/">The Marshalls 949,-</a></li>
                            </ul>
                    </ul>
                    <!-- navbar right -->
                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="False">Ud Af Huset<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="~/ud-af-huset/">Ud af Huset</a></li>
                                <li class="divider"></li>
                                <li><a href="~/the-lukes/">The Lukes 95,-</a></li>
                                <li><a href="~/the-luckylukes/">The LuckyLukes 295,-</a></li>
                                <li><a href="~/the-delukes/">The DeLukes 550,-</a></li>
                                <li><a href="~/cowboy-land/">Cowboy Land</a></li>
                            </ul>
                        </li>

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="False">Om Festen<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="~/dekoration-stemning/">Dekoration & Stemning</a></li>
                                <li><a href="~/aktiviteter-konkurencer/">Aktiviteter & Konkurencer</a></li>
                                <li><a href="~/show-underholdning/">Show & Underholdning</a></li>
                                <li><a href="~/buffe-bar/">Buffet & Bar</a></li>
                                <li><a href="~/program/">Program</a></li>
                                <li><a href="~/kostumer-diverse/">Kostumer & Diverse</a></li>
                                <li><a href="~/galleri/">Galleri</a></li>
                            </ul>
                        </li>
                        <li><a href="~/kontakt-bournonvilles/">Kontakt</a></li>
                    </ul>
                </div><!-- /.navbar-collapse -->
            </div><!-- /.container-fluid -->
        </nav>
    </header>

    @RenderBody()
    
    <div class="container-fluid footer-img" align="center">
        <div class="col-md-3">
            <a href="~/news-events/"><img src="~/media/Images/latestnews-events.png" alt="latestnews-events" height="100" width="200"></a>
        </div>
        <div class="col-md-3">
            <a href="~/cowboy-fest/"><img src="~/media/Images/cowboyfest.png" alt="cowboyfest" height="100" width="200"></a>
        </div>
        <div class="col-md-3">
            <a href="~/bestil-bord/"><img src="~/media/Images/booking.png" alt="bestil-bord" height="100" width="200"></a>
        </div>
        <div class="col-md-3">
            <a href="~/referencer/"><img src="~/media/Images/referencer.png" alt="referencer" height="100" width="200"></a>
        </div>
    </div>
    <footer>
        <div class="container-fluid footer">

            <div class="col-md-6 col-md-push-3 col-sm-12">
                <section class="related-links">
                    <p align="center">
                        <a href="~/om-bournonvilles/">Om Bournonvilles</a><br />
                        <a href="~/ledige-job/">Ledige Job</a> | <a href="~/news-events/nyhedsbrev/">Nyhedsbrev</a> <br />
                        <a href="~/samarbejds-partnere/">Samarbejds Partnere</a> | <a href="~/forretnings-betingelser/">Forretnings Betingelser</a><br />
                        Bournonvilles.dk &copy; @DateTime.Now.Year
                    </p>
                </section>
            </div>
            <div class="col-md-3 col-md-pull-6 col-xs-6">
                <aside>
                    <a href="http://www.findsmiley.dk/da-DK/Searching/DetailsView.htm?virk=91456286"><img src="~/media/Images/kontrol_raport.gif" /></a>
                </aside>
            </div>
            <div class="col-md-3 col-xs-6" align="right">
                <aside>
                    <a href="https://www.facebook.com/cowboyfest"><img src="~/media/Images/facebook.png" /></a>
                </aside>
            </div>
        </div>
    </footer>
    <!-- animated header -->
    <script src="~/Scripts/classie.js"></script>
    <script src="~/Scripts/cbpAnimatedHeader.js"></script>
    <script src="~/Scripts/jquery.nanogallery.min.js"></script>
    <script src="~/Scripts/nanoGallery.js"></script>
</body>
</html>
