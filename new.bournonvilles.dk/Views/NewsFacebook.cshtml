@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Master.cshtml";
}
<body>
@* Facebook script *@
    <div id="fb-root"></div>
    <script>
(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/da_DK/sdk.js#xfbml=1&version=v2.6&appId=1044427825650540";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
<section class="content">
    @CurrentPage.GetGridHtml("headlineGrid")
</section>
    <div class="row">
        <section class="col-lg-6 fb-news-section">
            @CurrentPage.GetGridHtml("contentGrid")
        </section>
        <section class="col-lg-6 col-lg-push-2 pull-right fb-section">
            <div class="fb-page" data-href="https://www.facebook.com/cowboyfest/?fref=ts" data-tabs="timeline, events, message" data-width="500px" data-small-header="false" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="true" data-height="2000px" align="right">
                <div class="fb-xfbml-parse-ignore">
                    <blockquote cite="https://www.facebook.com/cowboyfest/?fref=ts"><a href="https://www.facebook.com/cowboyfest/?fref=ts">Bournonvilles Wild West</a></blockquote>
                </div>
            </div>
        </section>
    </div>
</body>