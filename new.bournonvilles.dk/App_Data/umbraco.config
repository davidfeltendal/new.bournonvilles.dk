            {
              "grid": 4,
              "controls": [
                {
                  "value": "<p><a data-id=\"1209\" href=\"/{localLink:1209}\" title=\"Dekoration &amp; Stemning\"><img id=\"__mcenew\" src=\"/media/1082/dekostem1.jpg\" alt=\"\" rel=\"1154\" data-id=\"1154\" /></a></p>",
                  "editor": {
                    "alias": "rte"
